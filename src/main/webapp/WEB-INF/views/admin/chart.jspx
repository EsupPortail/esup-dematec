<div xmlns:sec="http://www.springframework.org/security/tags" xmlns:c="http://java.sun.com/jsp/jstl/core"  xmlns:field="urn:jsptagdir:/WEB-INF/tags/form/fields" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:page="urn:jsptagdir:/WEB-INF/tags/form"
    xmlns:spring="http://www.springframework.org/tags"
	>
    <jsp:directive.page contentType="text/html;charset=UTF-8"/>
	<jsp:output omit-xml-declaration="yes" />
	
    <spring:url value="/resources/js/Chart.min.js" var="chart_js_url" />
	<script src="${chart_js_url}" type="text/javascript"><!-- required for FF3 and Opera --></script>

	<div class="row-fluid">

		<div class="span4">
			<canvas id="uploadChart" width="400" height="400"><!--  --></canvas>
			
			 <script>
		
					var data = {
						    labels: ${uploadStatsLabels},
						    datasets: [
						        {
						            label: "Nombre de fichiers envoy√©s",
						            backgroundColor: "rgba(151,187,256,0.7)",
						            data: ${uploadStatsValues}
						        }
						    ]
						};
					
					var ctx = document.getElementById("uploadChart").getContext("2d");
					var myNewChart = new Chart(ctx, {type: 'line', data: data});
		
				</script>
			</div>
		
	</div>
		
</div>
