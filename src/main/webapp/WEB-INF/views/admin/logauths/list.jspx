<div xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:form="urn:jsptagdir:/WEB-INF/tags/form" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:page="urn:jsptagdir:/WEB-INF/tags/form" xmlns:table="urn:jsptagdir:/WEB-INF/tags/form/fields" version="2.0">
    <jsp:directive.page contentType="text/html;charset=UTF-8"/>
    <jsp:output omit-xml-declaration="yes"/>

	<div class="row-fluid">
		<div class="span12">
			<div class="accordion help" id="accordionfilter">
				<div class="accordion-group">
					<div class="accordion-heading">
						<a class="accordion-toggle" data-toggle="collapse" data-parent="#accordionfilter" href="#collapseFilter"> Filtrer </a>
					</div>
					<div id="collapseFilter" class="accordion-body collapse">
						<div class="accordion-inner">
							<div class="row-fluid">
								<form:find finderName="ByActionEquals" id="ff_fr_univrouen_poste_domain_LogAuth" path="/admin/logauths">
								
								    <c:forTokens var="action" items="AUTH SUCCESS;AUTH FAILED;AUTH PASSWORD CHANGED;AUTH PASSWORD FORGOT SENT;AUTH PASSWORD FORGOT FAILED" delims=";">
										<div class="span3">
											<c:choose>
												<c:when test="${param.action == action or param.action == null and action == 'AUTH SUCCESS'}">
													<input name="action" type="radio" value="${action}" checked="checked" />${action}
											      </c:when>
												<c:otherwise>
												    <input name="action" type="radio" value="${action}" />${action}
												</c:otherwise>
											</c:choose>
										</div>
									</c:forTokens>
									
								</form:find>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>

	<page:list id="pl_fr_univrouen_poste_domain_LogAuth" items="${logauths}" z="dyl+WI0gNmvcRslPjNrzmR+mveA=">
        <table:table create="false" data="${logauths}" delete="false" id="l_fr_univrouen_poste_domain_LogAuth" path="/admin/logauths" update="false" view="false" z="user-managed">
            <table:column date="true" dateTimePattern="${logAuth_actiondate_date_format}" id="c_fr_univrouen_poste_domain_LogAuth_actionDate" property="actionDate" z="VThjupWxMFpqaqemyZjt88xMwHw=" sort="true"/>
            <table:column id="c_fr_univrouen_poste_domain_LogAuth_userId" property="userId" z="q9X6VaKjvHi7KtKDZ5zwO6JculM=" sort="true"/>
            <table:column id="c_fr_univrouen_poste_domain_LogAuth_action" property="action" z="Szx5CMpfSqFsnteW0+msvTdxiSc=" sort="true"/>
            <table:column id="c_fr_univrouen_poste_domain_LogAuth_ip" property="ip" z="cHnrqk4ghd3LJvHDDDiiVnAl3lY="/>
        </table:table>
    </page:list>
</div>
