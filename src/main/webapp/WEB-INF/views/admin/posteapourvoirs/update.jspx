    			lists: true,
    			color: true,
    			html: true
    		}
    );
    
    $( "#_dateEndCandidatAuditionnable_id" ).datepicker({ dateFormat:'dd/mm/yy'}); 
    $( "#_dateEndCandidat_id" ).datepicker({ dateFormat:'dd/mm/yy'}); 
    $( "#_dateEndCandidatActif_id" ).datepicker({ dateFormat:'dd/mm/yy'}); 
    
});
