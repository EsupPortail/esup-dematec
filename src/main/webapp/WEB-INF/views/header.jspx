<header id="overview" class="subhead page-header" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:fn="http://java.sun.com/jsp/jstl/functions"
	xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:spring="http://www.springframework.org/tags" >
	<jsp:directive.page contentType="text/html;charset=UTF-8" />
	<jsp:output omit-xml-declaration="yes" />

	<div class="container-fluid">
		<spring:url var="banner" value="${imageUrl}"/>
		<spring:url var="home" value="/" />
		<spring:message code="button_home" var="home_label" htmlEscape="false" />
		<div class="row-fluid">
			
			<div class="span3 span-demat-left">
				<a href="${home}" name="${fn:escapeXml(home_label)}" title="${fn:escapeXml(home_label)}"> <img src="${banner}" />
				</a>
			</div>
			<div class="span9 span-demat-right">
				<h2>${title}</h2>
				<h4>${subTitle}</h4>
			</div>
		</div>
	</div>
</header>